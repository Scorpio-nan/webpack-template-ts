{
  "compilerOptions": {
    "outDir": "./",
    "noImplicitAny": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "es5",
    "lib": ["DOM","DOM.Iterable","ES2015","ES2018.Promise","ES2016.Array.Include"],
    "allowJs": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules","dist","webpack.config.js"],
  "include":["src"]
}